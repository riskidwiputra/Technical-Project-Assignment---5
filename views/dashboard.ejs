<div class="container-fluid">
                      
    <!-- start page title -->
    <div class="row">
        <div class="col-12">
            <div class="page-title-box">
               
                <h4 class="page-title">Task
                    <a href="#" data-toggle="modal" data-target="#add-new-task-modal" class="btn btn-success btn-sm ml-3">Add New</a></h4>
            </div>
        </div>
    </div>     
    <!-- end page title --> 
    <% let taskOne = addData.getStatusOne %>
    <div class="row">
        <div class="col-12">
            <div class="board">
                <div class="tasks" data-plugin="dragula" data-containers='["task-list-one", "task-list-two", "task-list-three", "task-list-four"]'>
                    <h5 class="mt-0 task-header">TODO (<%= taskOne.length %>)</h5>
                    
                    <div id="task-list-one" class="task-list-items">
                      
                        <% if(taskOne.length !== 0 ){ %>
                            <% taskOne.forEach(element => { %>
                              <!-- Task Item -->
                        <div class="card mb-0">
                            <div class="card-body p-3">
                                <small class="float-right text-muted"><% let date = element.createdAt; %><%= new Date(date).toDateString() %> </small>
                                <% if (element.label.name == "low") { %>
                                    <span class="badge badge-danger"><%= element.label.name %>
                                <% }else if(element.label.name == "medium"){ %>
                                    <span class="badge badge-info"><%= element.label.name %>
                                <% }else if(element.label.name == "high"){ %>
                                    <span class="badge badge-success"><%= element.label.name %>
                                <% } %>

                                </span>

                                <h5 class="mt-3 mb-2">
                                    <a href="#" data-toggle="modal" data-target="#task-detail-modal" data-id="<%= element.id %>" class="text-body detailTaks"><%= element.title.toUpperCase() %> </a>
                                </h5>

                                <p class="mt-2">

                                </p>

                                <div class="dropdown float-right">
                                    <a href="#" class="dropdown-toggle text-muted arrow-none" data-toggle="dropdown" aria-expanded="false">
                                        <i class="mdi mdi-dots-vertical font-18"></i>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right">
                                        <!-- item-->
                                        <a href="javascript:void(0);" class="dropdown-item"><i class="mdi mdi-pencil mr-1"></i>Edit</a>
                                        <!-- item-->
                                        <a href="javascript:void(0);" class="dropdown-item"><i class="mdi mdi-delete mr-1"></i>Delete</a>
                                       
                                    </div>
                                </div>

                                <p class="mb-0">
                                    <img src="assets/images/users/User.png" alt="user-img" class="avatar-xs rounded-circle mr-1" />
                                    <span class="align-middle"><%= element.assigned.User.fullname %> </span>
                                </p>
                            </div> <!-- end card-body -->
                        </div>
                        <!-- Task Item End -->
                            <% }) %>
                        <% } %>
                       
                        
                    </div> <!-- end company-list-1-->
                </div>
                <% let taskTwo = addData.getStatusTwo %>
                <div class="tasks">
                    <h5 class="mt-0 task-header text-uppercase">In Progress (<%= taskTwo.length %>)</h5>
                    
                    <div id="task-list-two" class="task-list-items">

                       
                        <% if(taskTwo.length !== 0 ){ %>
                            <% taskTwo.forEach(element => { %>
                              <!-- Task Item -->
                        <div class="card mb-0">
                            <div class="card-body p-3">
                                <small class="float-right text-muted"><% let date = element.createdAt; %><%= new Date(date).toDateString() %> </small>
                                <span class="badge badge-danger"><%= element.label.name %> </span>

                                <h5 class="mt-2 mb-2">
                                    <a href="#" data-toggle="modal" data-target="#task-detail-modal" class="text-body"><%= element.title %> </a>
                                </h5>

                                <p class="mt-3">

                                </p>

                                <div class="dropdown float-right">
                                    <a href="#" class="dropdown-toggle text-muted arrow-none" data-toggle="dropdown" aria-expanded="false">
                                        <i class="mdi mdi-dots-vertical font-18"></i>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right">
                                        <!-- item-->
                                        <a href="javascript:void(0);" class="dropdown-item"><i class="mdi mdi-pencil mr-1"></i>Edit</a>
                                        <!-- item-->
                                        <a href="javascript:void(0);" class="dropdown-item"><i class="mdi mdi-delete mr-1"></i>Delete</a>
                                       
                                    </div>
                                </div>

                                <p class="mb-0">
                                    <img src="assets/images/users/User.png" alt="user-img" class="avatar-xs rounded-circle mr-1" />
                                    <span class="align-middle">Robert Carlile</span>
                                </p>
                            </div> <!-- end card-body -->
                        </div>
                        <!-- Task Item End -->
                            <% }) %>
                        <% } %>

                    </div> <!-- end company-list-2-->
                </div>
                <% let taskThree = addData.getStatusThree %>
                <div class="tasks">
                    <h5 class="mt-0 task-header text-uppercase">Done (<%= taskThree.length %>)</h5>
                    <div id="task-list-four" class="task-list-items">

                       
                        <% if(taskThree.length !== 0 ){ %>
                            <% taskThree.forEach(element => { %>
                              <!-- Task Item -->
                        <div class="card mb-0">
                            <div class="card-body p-3">
                                <small class="float-right text-muted"><% let date = element.createdAt; %><%= new Date(date).toDateString() %> </small>
                                <span class="badge badge-danger">High</span>

                                <h5 class="mt-2 mb-2">
                                    <a href="#" data-toggle="modal" data-target="#task-detail-modal" class="text-body"><%= element.title %> </a>
                                </h5>

                                <p class="mt-3">

                                </p>

                                <div class="dropdown float-right">
                                    <a href="#" class="dropdown-toggle text-muted arrow-none" data-toggle="dropdown" aria-expanded="false">
                                        <i class="mdi mdi-dots-vertical font-18"></i>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right">
                                        <!-- item-->
                                        <a href="javascript:void(0);" class="dropdown-item"><i class="mdi mdi-pencil mr-1"></i>Edit</a>
                                        <!-- item-->
                                        <a href="javascript:void(0);" class="dropdown-item"><i class="mdi mdi-delete mr-1"></i>Delete</a>
                                       
                                    </div>
                                </div>

                                <p class="mb-0">
                                    <img src="assets/images/users/User.png" alt="user-img" class="avatar-xs rounded-circle mr-1" />
                                    <span class="align-middle">Robert Carlile</span>
                                </p>
                            </div> <!-- end card-body -->
                        </div>
                        <!-- Task Item End -->
                            <% }) %>
                        <% } %>
                        
                    </div> <!-- end company-list-4-->
                </div>

            </div> <!-- end .board-->
        </div> <!-- end col -->
    </div>
    <!-- end row-->
     <!--  Add new task modal -->
     <div class="modal fade task-modal-content" id="add-new-task-modal" tabindex="-1" role="dialog" aria-labelledby="NewTaskModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="NewTaskModalLabel">Create New Task</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                </div>
                <div class="modal-body">
                    <form class="p-2" action="/tasks/" method="post">
                        

                        <div class="row">
                            <div class="col-md-8">
                                <div class="form-group">
                                    <label for="task-title">Title</label>
                                    <input type="text" name="title" class="form-control form-control-light" id="task-title" placeholder="Enter title">
                                </div>
                            </div>

                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="task-priority2">Priority</label>
                                    <select class="form-control form-control-light" name="label" id="task-priority2">
                                        <option value="" readonly hidden> Priority</option>
                                        <% if(addData.length !== 0 ){ %>
                                            <% let priority = addData.label %> 
                                            <% priority.forEach(element => { %>
                                                
                                                <option value="<%= element.id %>"><%= element.name %></option>
                                            <% }) %>
                                        <% } %>
                                      
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="task-description">Description</label>
                            <textarea class="form-control form-control-light" name="description" id="task-description" rows="3"></textarea>
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="task-title">Assign To</label>
                                    <select class="form-control form-control-light" name="assign" id="task-priority">
                                        <option value="" readonly hidden> Assign To</option>
                                        <% if(addData.length !== 0 ){ %>
                                            <% let Assign = addData.users %> 
                                            <% Assign.forEach(element => { %>
                                                <option value="<%= element.id %>"><%= element.fullname %></option>
                                            <% }) %>
                                        <% } %>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="task-priority">Due Date</label>
                                    <input type="date" class="form-control form-control-light" name="due_date" >
                                </div>
                            </div>
                        </div>
                        
                        <div class="text-right">
                            <button type="button" class="btn btn-light" data-dismiss="modal">Cancel</button>
                            <button type="submit" class="btn btn-primary">Create</button>
                        </div>
                    </form>
                </div>
                
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->



         <!--  Task details modal -->
         <div class="modal fade task-modal-content" id="task-detail-modal" tabindex="-1" role="dialog" aria-labelledby="TaskDetailModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title" id="TaskDetailModalLabel"><span id="detail_title"></span> <span class="badge ml-2" id="detail_label"></span></h4>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                    </div>
                    <div class="modal-body">
                    
                        <div class="p-2">
                            <h5 class="mt-0">Description:</h5>
    
                            <p class="text-muted mb-4" id="detail_description">
                               
                            </p>
    
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="mb-4">
                                        <h5>Create Date</h5>
                                        <p id="detail_create_at"></p>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="mb-4">
                                        <h5>Due Date</h5>
                                        <p id="detail_dueDate"></p>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="mb-4">
                                        <h5>Asignee: </h5>
                                        <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="" data-original-title="Mat Helme" class="d-inline-block">
                                            <img src="assets/images/users/User.png" class="rounded-circle avatar-xs" alt="friend">
                                        </a>
                                        &nbsp;<span id="detail_asignee"></span>
                                    </div>
                                </div>
                            </div>
                            

                        </div> <!-- .p-2 -->
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
</div>
<script>
    $('.detailTaks').click(function(){
        var id_task = $(this).data('id');
        $.ajax({ 
            url      : "<%= process.env.BASEURL %>"+"tasks/"+id_task, 
            type     : 'GET', 
            dataType : 'JSON', 
            success  : function(data){
                $("#detail_title").html(data.title.toUpperCase());
                let color = ``;
                if(data.id_label == 1){
                    color += `badge-danger`;
                }else if(data.id_label == 2){
                    color += `badge-info`
                }else if(data.id_label == 3){
                    color += `badge-success`
                }
                $("#detail_label").html(data.label.name);
                $("#detail_label").addClass(color);
                $("#detail_description").html(data.description);
                let date = data.createdAt;
                date = new Date(date).toDateString()
                console.log(date);
                $("#detail_create_at").html(date);
                let due_date = data.due_date;
                due_date = new Date(due_date).toDateString()
                console.log(data.assigned.User);
                $("#detail_dueDate").html(due_date);
                $("#detail_asignee").html(data.assigned.User.fullname);
                
                
            },
            error: function() {
                console.log("gagal");
            }
        });
    });
</script>